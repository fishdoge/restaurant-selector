  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@types/express": "^4.17.17",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "prettier": "^2.8.8"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node ./app.ts",
